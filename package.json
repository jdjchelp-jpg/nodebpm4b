    },
    "keywords": [
        "mp3",
        "m4b",
        "audiobook",
        "converter",
        "ffmpeg"
    ],
    "author": "JDJCHELP",
    "email": "JDJCHELP@proton.me",
    "license": "MIT",
    "dependencies": {
        "commander": "^12.1.0",
