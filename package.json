        "multer": "^2.0.2",
        "uuid": "^9.0.1"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "repository": {
        "type": "git",
        "url": "."
    },
    "files": [
        "bin/",
        "lib/",
        "templates/",
